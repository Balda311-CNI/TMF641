    <bpmn2:task id="_AE3E7593-24D3-4FE0-89D8-C0C22AC51CCE" drools:taskName="Rest" name="Find service components">
      <bpmn2:documentation id="_tE11NHQtEe-8x6f_Rqupcw"><![CDATA[Finds service components of the service specified by the rfsCrossId.
 - Customer location
 - CPE
 - PE interface
 - Service link]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Find service components]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String rfsCrossId = (String) kcontext.getVariable("rfsCrossId");
String rfscCustomerLocationName = (String) kcontext.getVariable("rfscCustomerLocationName");
String rfscCPEName = (String) kcontext.getVariable("rfscCPEName");
String rfscPEInterfaceName = (String) kcontext.getVariable("rfscPEInterfaceName");
String rfscServiceLinkName = (String) kcontext.getVariable("rfscServiceLinkName");
Integer exactSearchLimit = (Integer) kcontext.getVariable("exactSearchLimit");

String url = java.lang.String.format(
    "%s/v1/exactSearch/",
    kcontext.getKieRuntime().getGlobal("crossApiBaseUrl")
);
kcontext.setVariable("apiUrl", url);

java.util.List<com.cross.ExactSearchJoinBody> joins = new java.util.ArrayList<>();
joins.add(new com.cross.ExactSearchJoinBody(
    "BASE",
    "serviceComponents",
    null,
    false
));
joins.add(new com.cross.ExactSearchJoinBody(
    "BASE__serviceComponents",
    "PRIMARY_SERVICE",
    null,
    false
));
joins.add(new com.cross.ExactSearchJoinBody(
    "BASE__serviceComponents__PRIMARY_SERVICE",
    "serviceComponents",
    java.lang.String.format("name = '%s' | name = '%s' | name = '%s' | name = '%s'",
                            rfscCustomerLocationName,
                            rfscCPEName,
                            rfscPEInterfaceName,
                            rfscServiceLinkName),
    false
));
java.util.List<com.cross.ExactSearchAttributeBody> select = new java.util.ArrayList<>();
select.add(new com.cross.ExactSearchAttributeBody(
    "BASE__serviceComponents__PRIMARY_SERVICE__serviceComponents",
    "name"
));
select.add(new com.cross.ExactSearchAttributeBody(
    "BASE__serviceComponents__PRIMARY_SERVICE__serviceComponents",
    "crossId"
));
java.util.List<com.cross.ExactSearchAttributeBody> orderBy = new java.util.ArrayList<>();
com.cross.ExactSearchBody body = new com.cross.ExactSearchBody(
    "SERVICE",
    java.lang.String.format("crossId = '%s'", rfsCrossId),
    joins,
    select,
    orderBy,
    true,
    exactSearchLimit);
kcontext.setVariable("apiBody", body);

System.out.println("POST API call: " + url + " " + body.toString());]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String rfsCrossId = (String) kcontext.getVariable("rfsCrossId");
String rfscCustomerLocationName = (String) kcontext.getVariable("rfscCustomerLocationName");
String rfscCPEName = (String) kcontext.getVariable("rfscCPEName");
String rfscPEInterfaceName = (String) kcontext.getVariable("rfscPEInterfaceName");
String rfscServiceLinkName = (String) kcontext.getVariable("rfscServiceLinkName");

Integer responseCode = (Integer) kcontext.getVariable("restStatus");
String  responseMessage = (String) kcontext.getVariable("restStatusMsg");
System.out.println("POST API response code: " + responseCode);
System.out.println("POST API response message: " + responseMessage);

if ( responseCode >= 200 && responseCode < 300 ) {
    String jsonStr = (String) kcontext.getVariable("restResult");
    System.out.println("POST API response result: " + jsonStr);

    org.json.JSONObject jsonObject = new org.json.JSONObject(jsonStr);
    org.json.JSONArray items = jsonObject.getJSONArray( "items" );
    for ( int i = 0; i < items.length(); i++ ) {
        org.json.JSONObject item = (org.json.JSONObject) items.get(i);
        String name = item.getString("BASE__serviceComponents__PRIMARY_SERVICE__serviceComponents/name");
        String crossId = item.getString("BASE__serviceComponents__PRIMARY_SERVICE__serviceComponents/crossId");
        if ( rfscCustomerLocationName.equals(name) ) {
            kcontext.setVariable("rfscCustomerLocationCrossId", crossId);
        }
        else if ( rfscCPEName.equals(name) ) {
            kcontext.setVariable("rfscCPECrossId", crossId);
        }
        else if ( rfscPEInterfaceName.equals(name) ) {
            kcontext.setVariable("rfscPEInterfaceCrossId", crossId);
        }
        else if ( rfscServiceLinkName.equals(name) ) {
            kcontext.setVariable("rfscServiceLinkCrossId", crossId);
        }
    }
}
else {
    int responseMessageJsonStartIndex = responseMessage.indexOf("{");
    String responseMessageJsonPart = responseMessage.substring(responseMessageJsonStartIndex);
    org.json.JSONObject responseMessageJsonObject = new org.json.JSONObject(responseMessageJsonPart);
    responseMessage = responseMessageJsonObject.getString("message");

    com.cross.ErrorBody body = new com.cross.ErrorBody(
        responseCode,
        responseMessage,
        kcontext.getNodeInstance().getNode().getName());
    kcontext.setVariable("error", body);

    System.out.println("Error: " + body.toString());
}

// validate result
String missingServiceComponentName = null;
if ( kcontext.getVariable("rfscCustomerLocationCrossId") == null ) {
    missingServiceComponentName = rfscCustomerLocationName;
}
else if ( kcontext.getVariable("rfscCPECrossId") == null ) {
    missingServiceComponentName = rfscCPEName;
}
else if ( kcontext.getVariable("rfscPEInterfaceCrossId") == null ) {
    missingServiceComponentName = rfscPEInterfaceName;
}
else if ( kcontext.getVariable("rfscServiceLinkCrossId") == null ) {
    missingServiceComponentName = rfscServiceLinkName;
}
if ( missingServiceComponentName != null ) {
    com.cross.ErrorBody body = new com.cross.ErrorBody(
        400,
        java.lang.String.format(
            "Unable to find service component '%s' in the service %s",
            missingServiceComponentName,
            rfsCrossId),
        kcontext.getNodeInstance().getNode().getName());
    kcontext.setVariable("error", body);

    System.out.println("Error: " + body.toString());
}]]></drools:script>
