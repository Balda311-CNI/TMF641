    <bpmn2:task id="_E9CFF418-8837-4E41-B07F-28E1BE75BF11" drools:taskName="Rest" name="Create access link">
      <bpmn2:documentation id="_tE2bc3QtEe-8x6f_Rqupcw"><![CDATA[Creates access link:
Link type: accessLinkLinkType
Name: Start node name --- End node name
Status: linkStatus
Description: accessLinkDescription 
Capacity: accessLinkCapacityBitPerSec
Start node: PE port created in the task "Create PE port"
End node:  CPE port created in the task "Create CPE"
Routing policy: fiberRoutingPolicy]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Create access link]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String projectName = (String) kcontext.getVariable("projectName");
String cpeCrossId = (String) kcontext.getVariable("cpeCrossId");
String cpeName = (String) kcontext.getVariable("cpeName");
String pePortCrossId = (String) kcontext.getVariable("pePortCrossId");
String pePortName = (String) kcontext.getVariable("pePortName");
String fiberRoutingPolicy = (String) kcontext.getVariable("fiberRoutingPolicy");
String accessLinkLinkType = (String) kcontext.getVariable("accessLinkLinkType");
String linkStatus = (String) kcontext.getVariable("linkStatus");
String accessLinkDescription = (String) kcontext.getVariable("accessLinkDescription");
String accessLinkCapacityBitPerSec = String.valueOf( kcontext.getVariable("accessLinkCapacityBitPerSec") );
String opticalPathCrossId = (String) kcontext.getVariable("opticalPathCrossId");

String url = java.lang.String.format(
    "%s/v1/link/?attributesToShow=%s&projectName=%s",
    kcontext.getKieRuntime().getGlobal("crossApiBaseUrl"),
    "crossId", // attributesToShow
    projectName
);
kcontext.setVariable("apiUrl", url);

String linkName = pePortName + " --- " + cpeName;
java.util.List<com.cross.CreateLinkProviderBody> providers = new java.util.ArrayList<>();
providers.add(
    new com.cross.CreateLinkProviderBody(
        opticalPathCrossId,
        "MAIN",
        false,
        1,
        1
    )
);
com.cross.CreateLinkBody body = new com.cross.CreateLinkBody(
    linkName,
    accessLinkDescription,
    Integer.parseInt(accessLinkCapacityBitPerSec),
    fiberRoutingPolicy,
    1,
    0,
    linkStatus,
    accessLinkLinkType,
    pePortCrossId,
    cpeCrossId,
    providers);
kcontext.setVariable("apiBody", body);

System.out.println("POST API call: " + url + " " + body.toString());]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[Integer responseCode = (Integer) kcontext.getVariable("restStatus");
String  responseMessage = (String) kcontext.getVariable("restStatusMsg");
System.out.println("POST API response code: " + responseCode);
System.out.println("POST API response message: " + responseMessage);

if ( responseCode >= 200 && responseCode < 300 ) {
    String jsonStr = (String) kcontext.getVariable("restResult");
    System.out.println("POST API response result: " + jsonStr);

    org.json.JSONObject jsonObject = new org.json.JSONObject(jsonStr);
    String crossId = jsonObject.getString( "crossId" );
    kcontext.setVariable("accessLinkCrossId", crossId);
}
else {
    int responseMessageJsonStartIndex = responseMessage.indexOf("{");
    String responseMessageJsonPart = responseMessage.substring(responseMessageJsonStartIndex);
    org.json.JSONObject responseMessageJsonObject = new org.json.JSONObject(responseMessageJsonPart);
    responseMessage = responseMessageJsonObject.getString("message");

    com.cross.ErrorBody body = new com.cross.ErrorBody(
        responseCode,
        responseMessage,
        kcontext.getNodeInstance().getNode().getName());
    kcontext.setVariable("error", body);

    System.out.println("Error: " + body.toString());
}]]></drools:script>
        </drools:onExit-script>
