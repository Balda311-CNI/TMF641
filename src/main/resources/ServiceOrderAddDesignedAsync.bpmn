        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String projectName = (String) kcontext.getVariable("projectName");
String rfscCrossId = (String) kcontext.getVariable("rfscServiceLinkCrossId");
//String stateSuccess = (String) kcontext.getVariable("stateSuccess");
//String stateFailed = (String) kcontext.getVariable("stateFailed");
//String pePortCrossId = (String) kcontext.getVariable("pePortCrossId");
//String pePortName = (String) kcontext.getVariable("pePortName");
com.cross.ErrorBody errorBody = (com.cross.ErrorBody) kcontext.getVariable("error");

String url = java.lang.String.format(
    "%s/v1/som/tmf641/notify/serviceOrderCreateEvent",
    kcontext.getKieRuntime().getGlobal("gcApiBaseUrl")
);
kcontext.setVariable("apiUrl", url);

String state;
com.cross.NotifyErrorMessageBody errorMessage;
if ( errorBody == null ) {
    state = (String) kcontext.getVariable("stateSuccess");
    errorMessage = null;
}
else {
    state = (String) kcontext.getVariable("stateFailed");
    errorMessage = new com.cross.NotifyErrorMessageBody(
        "ErrorMessage",
        errorBody.getCode().toString(),
        errorBody.getMessage(),
        errorBody.getTask()
    );
}
com.cross.NotifyServiceOrderBody event = new com.cross.NotifyServiceOrderBody(
    "ServiceOrder",
    rfscCrossId,
    errorMessage,
    state,
    pePortName
);
com.cross.NotifyServiceOrderCreateEventBody body = new com.cross.NotifyServiceOrderCreateEventBody(
    event,
    "ServiceOrderCreateEvent",
    "ServiceOrderCreateEvent"
);
kcontext.setVariable("apiBody", body);

System.out.println("POST API call: " + url + " " + body.toString());]]></drools:script>
