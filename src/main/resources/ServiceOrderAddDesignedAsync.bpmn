    <bpmn2:task id="_816080BC-C569-40ED-B3C8-B0F00FF5EBC0" drools:taskName="Rest" name="Find PE port">
      <bpmn2:documentation id="_M7sKLHkTEe-bDKJddmkugw"><![CDATA[Finds physical/logical port under the PE transceiver 'peTransceiverCrossId' created in the 'Create PE transceiver' task.]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Find PE port]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String peTransceiverCrossId = (String) kcontext.getVariable("peTransceiverCrossId");
//String physicalPortNodeType = (String) kcontext.getVariable("physicalPortNodeType");
//String logicalPortNodeType = (String) kcontext.getVariable("logicalPortNodeType");
//Integer exactSearchLimit = (Integer) kcontext.getVariable("exactSearchLimit");

String url = java.lang.String.format(
    "%s/v1/exactSearch/",
    kcontext.getKieRuntime().getGlobal("crossApiBaseUrl")
);
kcontext.setVariable("apiUrl", url);

java.util.List<com.cross.ExactSearchJoinBody> joins = new java.util.ArrayList<>();
joins.add(new com.cross.ExactSearchJoinBody(
    "BASE",
    "DIRECT_CHILDREN",
    java.lang.String.format(
        "nodeType = '%s' & nodeType = '%s'",
        physicalPortNodeType,
        logicalPortNodeType),
    false
));
java.util.List<com.cross.ExactSearchAttributeBody> select = new java.util.ArrayList<>();
select.add(new com.cross.ExactSearchAttributeBody(
    "BASE__DIRECT_CHILDREN",
    "crossId"
));
select.add(new com.cross.ExactSearchAttributeBody(
    "BASE__DIRECT_CHILDREN",
    "name"
));
java.util.List<com.cross.ExactSearchAttributeBody> orderBy = new java.util.ArrayList<>();
com.cross.ExactSearchBody body = new com.cross.ExactSearchBody(
    "NODE",
    java.lang.String.format("crossId = '%s'", peTransceiverCrossId),
    joins,
    select,
    orderBy,
    true,
    exactSearchLimit);
kcontext.setVariable("apiBody", body);

System.out.println("POST API call: " + url + " " + body.toString());]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[//String peTransceiverCrossId = (String) kcontext.getVariable("peTransceiverCrossId");
//String physicalPortNodeType = (String) kcontext.getVariable("physicalPortNodeType");
//String logicalPortNodeType = (String) kcontext.getVariable("logicalPortNodeType");

Integer responseCode = (Integer) kcontext.getVariable("restStatus");
String  responseMessage = (String) kcontext.getVariable("restStatusMsg");
System.out.println("POST API response code: " + responseCode);
System.out.println("POST API response message: " + responseMessage);

if ( responseCode >= 200 && responseCode < 300 ) {
    String jsonStr = (String) kcontext.getVariable("restResult");
    System.out.println("POST API response result: " + jsonStr);

    org.json.JSONObject jsonObject = new org.json.JSONObject(jsonStr);
    org.json.JSONArray items = jsonObject.getJSONArray( "items" );

    String errorMessage = null;
    if ( items.length() == 0 ) {
        errorMessage = java.lang.String.format(
            "No %s/%s found under the PE transceiver %s",
            physicalPortNodeType,
            logicalPortNodeType,
            peTransceiverCrossId);
    }
    else if ( items.length() > 1 ) {
        errorMessage = java.lang.String.format(
            "No unique %s/%s found under the PE transceiver %s",
            physicalPortNodeType,
            logicalPortNodeType,
            peTransceiverCrossId);
    }

    if (errorMessage == null) {
        org.json.JSONObject item = (org.json.JSONObject) items.get(0);
        String crossId = item.getString("BASE__DIRECT_CHILDREN/crossId");
        String name = item.getString("BASE__DIRECT_CHILDREN/name");
        kcontext.setVariable("pePortCrossId", crossId);
        kcontext.setVariable("pePortName", name);
    }
